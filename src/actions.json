    {
        "key": "ssh_mysql_restart",
        "name": "Restart MySQL",
        "interactive": false,
        "commands": [
            "sudo systemctl restart mysql || sudo service mysql restart"
        ]
    },
    {
        "key": "ssh_apache_restart",
        "name": "Restart Apache",
        "interactive": false,
        "commands": [
            "sudo systemctl restart apache2",
            "sudo apache2ctl configtest",
            "sleep 5"
        ]
    },
